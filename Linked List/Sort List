https://leetcode.com/problems/sort-list/
Code:

Approach 1: Time Complexity O(nlogn) | Space Complexity O(n)
class Solution {
public:
    ListNode* sortList(ListNode* head) {
        if(head == NULL || head->next == NULL) return head;
        vector<int> v;
        int i = 0;
        ListNode* temp = head;
        while(temp){
            v.push_back(temp->val);
            temp = temp->next;
        }
        sort(v.begin(), v.end());
        temp = head;
        while(temp){
            temp->val = v[i++];
            temp = temp->next;
        }
        return head;
    }
};

