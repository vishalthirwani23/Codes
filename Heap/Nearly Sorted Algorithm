https://practice.geeksforgeeks.org/problems/nearly-sorted-algorithm/0
Code:

#include<bits/stdc++.h>
using namespace std;
int main()
 {
	int t;
	cin>>t;
	while(t--){
	int n,k;
	cin>>n>>k;
	int a[n],index=0;
	for(int i=0;i<n;i++)
	cin>>a[i];
	priority_queue<int,vector<int>,greater<int>> pq;
	for(int i=0;i<=min(n-1,k);i++)
	pq.push(a[i]);
	for(int i=k+1;i<n;i++){
	    a[index++]=pq.top();
	    pq.pop();
	    pq.push(a[i]);
	}
	while(!pq.empty()){
	    a[index++]=pq.top();
	    pq.pop();
	}
	for(int i=0;i<n;i++)
	cout<<a[i]<<" ";
	cout<<"\n";
	}
	return 0;
}
