https://leetcode.com/problems/binary-tree-preorder-traversal/
Code:

class Solution {
public:
    
    vector<int> preorderTraversal(TreeNode* root) {
        vector<int> ans;
        stack<TreeNode*> s;
        TreeNode *temp=root;
        while(s.empty()==false || temp!=NULL){
            while(temp){
                s.push(temp);
                ans.push_back(temp->val);
                temp=temp->left;
            }
           
                temp=s.top();
                s.pop();
                temp=temp->right;
            
        }
        return ans;
        
    }
};
